<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="user.dao.UserDao">
	<resultMap id="BaseResultMap" type="vo.EnvList">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="env_name" property="envName" jdbcType="VARCHAR" />
		<result column="env_bo" property="envBo" jdbcType="VARCHAR" />
		<result column="api_cfg" property="apiCfg" jdbcType="VARCHAR" />
		<result column="api_vdp" property="apiVdp" jdbcType="VARCHAR" />
		<result column="api_org" property="apiOrg" jdbcType="VARCHAR" />
		<result column="api_cas" property="apiCas" jdbcType="VARCHAR" />
		<result column="api_cfg_key" property="apiCfgKey" jdbcType="VARCHAR" />
		<result column="api_vdp_key" property="apiVdpKey" jdbcType="VARCHAR" />
		<result column="api_org_key" property="apiOrgKey" jdbcType="VARCHAR" />
		<result column="api_cas_key" property="apiCasKey" jdbcType="VARCHAR" />
		<result column="cfg_hosts" property="cfgHosts" jdbcType="CLOB" />
		<result column="tpl_pom" property="tplPom" jdbcType="CLOB" />
		<result column="tpl_java" property="tplJava" jdbcType="CLOB" />
		<result column="tpl_php" property="tplPHP" jdbcType="CLOB" />
		<result column="open_visibility" property="openVisibility"
			jdbcType="BIT" />
		<result column="open_registry" property="openRegistry"
			jdbcType="BIT" />
		<result column="create_time" property="createTime" jdbcType="TIMESTAMP" />
		<result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
		<result column="create_by" property="createBy" jdbcType="VARCHAR" />
		<result column="update_by" property="updateBy" jdbcType="VARCHAR" />
	</resultMap>
	<sql id="Base_Column_List">
		id, env_name, env_bo, api_cfg, api_vdp, api_org, api_cas,
		api_cfg_key,
		api_vdp_key,cfg_hosts,
		api_org_key,
		api_cas_key,tpl_pom,tpl_java,tpl_php,
		open_visibility,
		open_registry,
		create_time,
		update_time,
		create_by,
		update_by
	</sql>
	<select id="getUserInfo" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		from env_list
		where id = #{id,jdbcType=INTEGER}
	</select>
</mapper>